#include <stdio.h>
#include<string.h>

int main()
{
    int i,n,c=0;
    char s[100];
    printf("enter the number of string\n");
    
    gets(s);
    n=strlen(s);
    for(i=0;i<n/2;i++){
        if(s[i]==s[n-i-1])
        c++;
    }
            if(c==i){
            printf("string is palindrome\n");
            
        }
        else
        {
            printf("String is not palindrome\n");
    
        }

    return 0;
}
